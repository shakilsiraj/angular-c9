{
  "name": "angular-c9",
  "version": "1.0.0",
  "description": "Angular application development skeleton with TypeScript, Jasmine, Karma and Protractor at c9.io",
  "main": "index.ts",
  "scripts": {
    "rollup:ng2": "node_modules/.bin/rollup -c angular-imports.rollup.config.js",
    "rollup:app": "node_modules/.bin/rollup -c app.rollup.config.js",
    "compile:debug": "node_modules/.bin/tsc -p tsconfig.debug",
    "test": "node_modules/.bin/trash jasmine && npm run compile:jasmine && npm run test:jasmine",
    "test:jasmine": "node_modules/.bin/jasmine-ts",
    "compile:jasmine": "node_modules/.bin/tsc -p tsconfig.jasmine.json",
    "test:karma": "node_modules/.bin/karma start --no-browsers karma.config.js",
    "build:merge-startup-libs:min": "node_modules/.bin/uglifyjs node_modules/core-js/client/shim.min.js node_modules/zone.js/dist/zone.min.js node_modules/systemjs/dist/system.js node_modules/rxjs/bundles/Rx.min.js -o dist/startup.libs.min.js",
    "build:app": "npm run rollup:ng2 && npm run rollup:app && npm run build:merge-startup-libs:min",
    "copy:statics": "node_modules/.bin/cpx 'source/*.{html,htm,png,jpg,gif,css,ico}' dist",
    "dist": "node_modules/.bin/trash dist && npm run build:app && npm run copy:statics"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shakilsiraj/angular-c9.git"
  },
  "keywords": [
    "Angular",
    "TypeScript",
    "Jasmine",
    "Karma",
    "Protractor",
    "AOT"
  ],
  "author": "Shakil Siraj",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shakilsiraj/angular-c9/issues"
  },
  "homepage": "https://github.com/shakilsiraj/angular-c9#readme",
  "dependencies": {
    "@angular/common": "^4.1.0",
    "@angular/compiler": "^4.1.0",
    "@angular/core": "^4.1.0",
    "@angular/forms": "^4.1.0",
    "@angular/http": "^4.1.0",
    "@angular/platform-browser": "^4.1.0",
    "@angular/platform-browser-dynamic": "^4.1.0",
    "@angular/platform-webworker": "^4.1.0",
    "@angular/router": "^4.1.0",
    "angular-in-memory-web-api": "^0.3.1",
    "core-js": "^2.4.1",
    "rxjs": "^5.1.0",
    "systemjs": "^0.19.47",
    "zone.js": "^0.8.5"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^4.1.0",
    "@types/core-js": "^0.9.40",
    "@types/jasmine": "^2.5.46",
    "cpx": "^1.5.0",
    "jasmine-core": "^2.6.1",
    "jasmine-node": "^1.14.5",
    "jasmine-spec-reporter": "^4.0.0",
    "jasmine-ts": "^0.1.0",
    "jsdom": "^10.0.0",
    "karma": "^1.5.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-spec-reporter": "0.0.31",
    "karma-typescript": "^3.0.1",
    "karma-typescript-angular2-transform": "^1.0.0-beta.6",
    "karma-typescript-es6-transform": "^1.0.0",
    "rollup": "^0.41.6",
    "rollup-plugin-angular": "^0.4.4",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^3.2.2",
    "trash-cli": "^1.4.0",
    "typescript": "^2.3.2",
    "typings": "^2.1.0",
    "uglifyjs": "^2.4.10"
  }
}
